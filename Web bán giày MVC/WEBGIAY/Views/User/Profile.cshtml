@model WEBGIAY.Models.nguoiDung
@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<div class="container w-100 text-xl-left fs-2" style="color: sandybrown">HỒ SƠ CÁ NHÂN</div>


<div class="container w-75">

    <P class="card-text text-black">TÊN NGƯỜI DÙNG: @Html.DisplayFor(model => model.tenNguoiDung)</P>
    <P class="card-text text-black">EMAIL: @Html.DisplayFor(model => model.eMail)</P>
    <P class="card-text text-black">SỐ ĐIỆN THOẠI: @Html.DisplayFor(model => model.dienThoai)</P>
    <P class="card-text text-black">ĐỊA CHỈ: @Html.DisplayFor(model => model.diaChi)</P>

    <button class="btn btn-primary">
        @Html.ActionLink("Chỉnh sửa hồ sơ", "Edit", new { id = Model.maNguoiDung }, new { @style = "color:white;" })
    </button>
</div>

